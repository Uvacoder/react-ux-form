{
  "name": "react-ux-form-example",
  "version": "0.0.0",
  "scripts": {
    "build": "tsc && vite build",
    "clean": "rm -rf ./node_modules",
    "clean-modules": "rm -rf ./node_modules/react-ux-form/{example,node_modules}",
    "dev": "vite",
    "postinstall": "yarn clean-modules",
    "serve": "vite preview",
    "update": "yarn clean && cd .. && yarn build && cd example && yarn"
  },
  "dependencies": {
    "@chakra-ui/react": "^1.4.1",
    "@emotion/react": "^11",
    "@emotion/styled": "^11",
    "framer-motion": "^4",
    "react": "^17.0.2",
    "react-dom": "^17.0.2",
    "react-ux-form": "../",
    "validator": "^13.5.2",
    "wouter": "^2.7.4"
  },
  "devDependencies": {
    "@types/react": "^17.0.3",
    "@types/react-dom": "^17.0.3",
    "@vitejs/plugin-react-refresh": "^1.3.1",
    "typescript": "^4.2.3",
    "vite": "^2.1.2"
  }
}
